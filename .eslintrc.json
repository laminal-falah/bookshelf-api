{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
      "no-console": "off",
      "arrow-parens": "off",
      "no-restricted-globals": "off",
      "radix": "off"
    }
}
